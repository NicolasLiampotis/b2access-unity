<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pl.edu.icm.unity.db.mapper.AttributesMapper">

	<insert id="insertAttributeType" parameterType="BaseB">
		INSERT INTO ATTRIBUTE_TYPES (NAME, CONTENTS) VALUES (#{name}, #{contents})
	</insert>	

	<select id="getAttributeTypes" resultType="BaseB">
		select * from ATTRIBUTE_TYPES
	</select>

	<select id="getAttributeType" parameterType="string" resultType="BaseB">
		select * from ATTRIBUTE_TYPES WHERE NAME = #{value}
	</select>
<!-- 
	<update id="updateIdentityType" parameterType="BaseB">
		UPDATE IDENTITY_TYPES SET NAME = #{name}, CONTENTS=#{contents}
	</update>	

	
 -->
	
</mapper>